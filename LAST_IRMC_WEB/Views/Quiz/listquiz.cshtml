@model IEnumerable<Last_Irmc_Domain.quiz>

@{
    ViewBag.Title = "listquiz";
}

<h2>listquiz</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.quiz_nom)
        </th>
        <th></th>
    </tr>

@foreach (var item in ViewBag.result) {
    <tr>
        <td>
            @item.quiz_nom
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.quiz_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.quiz_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.quiz_id })
        </td>
    </tr>
}

</table>
